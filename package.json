{
  "name": "translifeline-home",
  "version": "1.0.0",
  "description": "Trans Lifeline's public facing page",
  "main": "app.js",
  "scripts": {
    "sass": "node-sass styles/ -o public/styles/",
    "prestart": "npm run sass",
    "start": "node app.js",
    "sass-watch": "node-sass -w styles/ -o public/styles/",
    "start-dev": "export NODE_ENV=development && parallelshell 'npm run sass-watch' 'nodemon app.js'"
  },
  "author": "Nina Chaubal",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "node-normalize-scss": "^1.3.2",
    "node-sass": "^3.8.0",
    "pug": "^2.0.0-beta3",
    "serve-favicon": "^2.3.0",
    "susy": "^2.2.12"
  },
  "devDependencies": {
    "express-livereload": "0.0.24",
    "nodemon": "^1.9.2",
    "parallelshell": "^2.0.0"
  }
}
